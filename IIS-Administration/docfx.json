{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*swagger.json"
        ],
        "exclude": [
          "**/obj/**",
          "IIS-Administration/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "IIS-Administration/**",
          "**/includes/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {},
    "fileMetadata": {},
    "template": [],
    "dest": "_site",
    "markdownEngineName": "markdig"
  }
}
